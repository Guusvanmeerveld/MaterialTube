{
	"trailingComma": "none",
	"useTabs": true,
	"semi": true,
	"printWidth": 80,
	"arrowParens": "always",
	"plugins": ["@ianvs/prettier-plugin-sort-imports"],
	"importOrder": [
		".*s?css$",
		"",
		"<THIRD_PARTY_MODULES>",
		"",
		"^(next/.*)|(react.*)",
		"",
		"^@(:?mui|nextui-org)/.*",
		"",
		"^@/hooks/.*",
		"",
		"^@/(client)|(utils)/.*",
		"",
		"^(@/|\\./)components/*",
		"",
		"^\\.?\\./.*",
		"",
		"<TYPES>",
		"",
		"^(@/|\\./)typings/.*"
	]
}
