{
	"trailingComma": "none",
	"useTabs": true,
	"semi": true,
	"printWidth": 80,
	"arrowParens": "always",
	"importOrderSeparation": true,
	"importOrder": [
		"^next.*",
		"^react$",
		"^react.*",
		"^@emotion/.*",
		"^@mui/material/.*",
		"^@mui/.*",
		"^@src/.*",
		"^@models/.*",
		"^@interfaces/.*",
		"^@utils/.*",
		"^@components/.*",
		".*sass$",
		".*css$",
		"^@svg/.*"
	]
}
